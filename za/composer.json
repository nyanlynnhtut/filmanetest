{
    "name": "za/support",
    "description": "Support package for za's laravel projects",
    "authors": [
        {
            "name": "Nyan Lynn Htut",
            "email": "lynnhtut87@gmail.com"
        }
    ],
    "homepage": "https://github.com/suenwe/za-support",
    "keywords": [
        "Laravel",
        "Support"
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/auth": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Za\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Za\\Support\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Za\\Support\\SupportServiceProvider"
            ]
        }
    }
}
